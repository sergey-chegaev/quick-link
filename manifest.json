{
  "manifest_version": 3,
  "name": "Quick link",
  "version": "0.0.2",
  "permissions": ["tabs", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
