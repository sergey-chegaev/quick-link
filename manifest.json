{
  "manifest_version": 2,
  "name": "Quick link",
  "version": "0.1",
  "permissions": ["tabs", "storage"],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    }
  }
}
